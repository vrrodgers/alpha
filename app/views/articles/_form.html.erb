
<%= render 'shared/errors', obj: @article %>



<%= form_for @article do |f| %>
	<p>
		<%= f.label :title %>
		<%= f.text_field :title %>
    </p>

    <p>
		<%= f.label :description %>
		<%= f.text_area :description %>
    </p>

    <p>
    	<%= f.submit %>
    </p>
<%end%>
<%= link_to "Back to articles listings", articles_path %>